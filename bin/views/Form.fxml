<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="183.0" prefWidth="444.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Controller">
   <children>
      <VBox prefHeight="200.0" prefWidth="220.0" spacing="20.0" StackPane.alignment="TOP_CENTER">
         <children>
            <JFXTextField fx:id="name" labelFloat="true" promptText="Project Name">
               <font>
                  <Font name="Comic Sans MS" size="14.0" />
               </font></JFXTextField>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <JFXTextField fx:id="name1" labelFloat="true" prefHeight="43.0" prefWidth="315.0" promptText="Absolute Path to Project" HBox.hgrow="ALWAYS">
                     <font>
                        <Font name="Comic Sans MS" size="14.0" />
                     </font>
                  </JFXTextField>
                  <Button mnemonicParsing="false" onAction="#onConfirmClicked" prefHeight="34.0" prefWidth="86.0" style="-fx-background-color: #880E4F;" text="Browse" textFill="WHITE">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#onConfirmClicked" style="-fx-background-color: #2196F3;" text="Confirm" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
         </padding>
      </VBox>
   </children>
</StackPane>
